        gap: 10px;
        /* Tighter gap */
    }

    .hero-actions-left .btn {
        width: 100%;
        justify-content: center;
    }

    /* Adjust Visuals for Phone */
    .hero-main-image-wrapper {
        height: 320px;
        /* Shorter container */
    }

    .hero-collage-img {
        width: 110px;
        height: 140px;
        bottom: -15px;
        right: 0px;
        border-width: 3px;
    }

    .floating-card {
        padding: 8px 12px;
        font-size: 13px;
    }

    .brand-logo-text {
        font-size: 13px;
    }

    /* Shift cards to be strictly within bounds */
    .card-1 {
        left: 0;
        top: 10px;
    }

    .card-3 {
        right: 0;
    }

    /* Ensure image wrapper doesn't overflow */
    .hero-main-image-wrapper {
        width: 100%;
        overflow: hidden;
        /* Clip anything sticking out */
        border-radius: 32px;
        /* Match internal radius */
    }
}

