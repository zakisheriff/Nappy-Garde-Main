    }

    .hero-headline-large {
        font-size: 42px;
    }

    .hero-actions-left {
        justify-content: center;
    }

    .hero-main-image-wrapper {
        height: 400px;
    }

    .hero-main-image {
        width: 100%;
        left: 0;
    }

    .hero-collage-img {
        width: 140px;
        height: 180px;
        bottom: -10px;
        right: 10px;
    }

    .floating-card {
        padding: 10px 16px;
    }

    .card-1 {
        left: -10px;
        top: 20px;
    }

    .card-2 {
