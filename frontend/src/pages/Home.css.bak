/* 9. Bundles Banner */
.bundle-banner {
    background: linear-gradient(135deg, #0071e3 0%, #4facfe 100%);
    border-radius: 32px;
    padding: 24px;
    color: #fff;
    text-align: center;
    position: relative;
    overflow: hidden;
}

/* 11. Delivery Icons */
.delivery-bar {
    display: flex;
    justify-content: space-around;
    padding: 60px 0;
    border-top: 1px solid #eee;
    margin-top: 60px;
}

.delivery-item {
    text-align: center;
    color: #1d1d1f;
}

/* Responsive Adjustments */
@media (max-width: 768px) {
    .marquee-content {
        gap: 16px;
    }

    .review-card {
        min-width: 280px;
        max-width: 280px;
        padding: 24px;
    }

    .hero-section {
        height: auto;
        min-height: auto;
        /* Let content dictate height */
        padding: 40px 0;
        margin: 10px;
        border-radius: 24px;
        overflow: hidden;
        /* CRITICAL: Clip aurora and visuals */
    }

    /* GLOBAL APP CONTAINER FIX */
    .app,
    .main-content,
    .page {
        width: 100%;
        max-width: 100vw;
        overflow-x: hidden;
    }

    /* FIX: Hide Aurora blobs completely on small screens to remove risk */
    .hero-aurora-1,
    .hero-aurora-2 {
        display: none !important;
    }

    /* Clip Hero Visuals strictly */
    .hero-visuals-right {
        max-width: 100%;
        overflow: hidden;
    }

    /* ... typography overrides ... */

    /* Fix Floating Cards Overflow */
    .card-1 {
        left: 0;
        /* Was -10px, fix to edge */
        top: 10px;
    }

    .card-3 {
        right: 0;
        /* Was -5px, fix to edge */
    }

    /* Ensure Containers don't bust width */
    .section-container,
    .hero-container-split {
        width: 100%;
        max-width: 100vw;
        box-sizing: border-box;
        padding-left: 20px !important;
        padding-right: 20px !important;
        overflow-x: hidden;
    }

    .hero-content {
        padding: 0;
        /* Remove extra padding causing width issues */
    }

    /* Fix Marquee Bleed */
    .marquee-wrapper {
        width: 100vw;
        margin-left: -20px;
        /* Counteract padding */
        mask-image: linear-gradient(to right, transparent, black 5%, black 95%, transparent);
    }

    /* Reduce Global Headlines on Mobile */
    .section-headline {
        font-size: 28px;
        /* Was 40px */
        letter-spacing: -0.5px;
        margin-bottom: 24px;
    }

    /* Reduce Hero Text further */
    .hero-headline {
        font-size: 32px;
        /* Was 36px/64px */
        margin-bottom: 12px;
    }

    .hero-subtext {
        font-size: 16px;
        /* Was 18px/24px */
        margin-bottom: 24px;
    }

    .hero-content {
        padding: 0 20px;
    }

    .hero-actions {
        flex-direction: column;
        width: 100%;
        gap: 12px;
    }

    .hero-actions .btn {
        width: 100%;
        justify-content: center;
        padding: 12px 24px;
        /* Slightly smaller padding */
        font-size: 15px;
    }

    /* Tighten Badges */
    .hero-trust-badges.centered-badges {
        gap: 12px;
    }

    .trust-badge-item {
        font-size: 12px;
        /* Smaller text */
        padding: 6px 12px;
    }

    /* Split Section Stacking */
    .split-section {
        grid-template-columns: 1fr;
        gap: 16px;
    }

    .split-card {
        height: auto;
        min-height: 380px;
        /* Reduce minimal height */
        padding: 32px;
    }

    .split-card h2 {
        font-size: 28px !important;
        /* Override inline style if any or css */
    }

    .split-card p {
        font-size: 16px !important;
    }

    .delivery-bar {
        flex-direction: column;
        gap: 32px;
        padding: 40px 0;
    }

    .bundle-banner {
        padding: 32px 20px;
    }
}

/* Phones specific overrides (Extra Tight) */
@media (max-width: 480px) {
    /* ... existing hero 2.0 overrides ... */

    /* Global Section Padding Reduction */
    .section-container {
        padding: 40px 16px;
        /* Reduce from 80px 0 */
        width: 100%;
    }

    .product-card-min {
        min-width: 220px;
        /* Smaller cards */
        padding: 16px;
    }

    /* Stage Cards */
    .stage-card {
        height: 240px;
        /* Was 300px/400px */
    }

    .stage-title {
        font-size: 20px;
        /* Was 24px */
    }

    .stage-content {
        bottom: 20px;
        left: 20px;
    }

    /* Trust Grid */
    .trust-card {
        padding: 24px;
    }

    .trust-card h3 {
        font-size: 16px;
        /* Smaller headers */
    }

    .trust-icon-large {
        width: 48px;
        height: 48px;
    }
}